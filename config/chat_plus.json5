{
  //Whether the users require operator for bold/italic/etc. (formatted) text
  "require_op_for_formatting": true,
  //Whether the users require operator for colored text
  "require_op_for_colors": false,
  //Whether the users require operator to use [item]
  "require_op_for_item_hover": false,
