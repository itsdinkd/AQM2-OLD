  "configEnchantments": [
    {
      "enchantmentId": "minecraft:silktouch",
      "personalFactor": 5.0,
      "activated": true
    },
    {
      "enchantmentId": "veinmining:vein_mining",
      "personalFactor": 7.0,
      "activated": true
    }
  ]
