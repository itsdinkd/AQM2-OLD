{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 2,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledChat#configuration",
  "defaultStyle": {
    "displayName": "${vanillaDisplayName}",
    "chat": "<yellow>${player}</yellow>: ${message}<reset>",
    "join": "<light_purple>✚</light_purple> <color:#85ff8f><lang:multiplayer.player.joined:'${player}'>",
    "joinRenamed": "<gray>✚</gray> <color:#85ff8f><lang:multiplayer.player.joined.renamed:'${player}':'${old_name}'>",
    "joinFirstTime": "<gray>✚</gray> <gradient:red:blue>[First Time Joining]</gradient> <gray>✚</gray> <color:#ff8585><lang:multiplayer.player.joined:'${player}'>",
    "left": "<color:#ff8585><lang:multiplayer.player.left:'${player}'>",
    "death": "<gray>☠</gray> <color:#d1d1d1>${default_message}",
    "advancementTask": "<lang:chat.type.advancement.task:'${player}':'${advancement}'>",
    "advancementChallenge": "<lang:chat.type.advancement.challenge:'${player}':'${advancement}'>",
    "advancementGoal": "<lang:chat.type.advancement.goal:'${player}':'${advancement}'>"
  },
  "permissionStyles": [
    {
      "permission": "group.admin",
      "style": {
        "displayName": "<dark_gray><bold>[<gradient:light_purple:red>AD<strikethrough>M</strikethrough>IN<reset><dark_gray><bold>]<reset> <gray>${vanillaDisplayName}</gray>",
        "chat": "${player} <dark_gray>»</dark_gray> <#e67c19>${message}<reset>"
      }
    },
    {
      "permission": "group.mod",
      "style": {
        "displayName": "<dark_gray><bold>[<gradient:gold:white>M<strikethrough>O</strikethrough>D<reset><dark_gray><bold>]<reset> <gray>${vanillaDisplayName}</gray>",
        "chat": "${player} <dark_gray>»</dark_gray> <gold>${message}<reset>"
      }
    },
    {
      "permission": "group.support",
      "style": {
        "displayName": "<#9026d9><bold>[<gradient:#ffe09f:#4db2a0>SUP<strikethrough>P</strikethrough>ORT<reset><#9026d9><bold>]<reset> <gray>${vanillaDisplayName}</gray>",
        "chat": "${player} <dark_gray>»</dark_gray> <gold>${message}<reset>"
      }
    },
    {
      "permission": "group.superdonor",
      "style": {
        "displayName": "<#0b027c><bold>[<gradient:#558fee:#b556ff>Super Donor</gradient><reset><#0b027c><bold>]<reset> <gold>${vanillaDisplayName}</gold><reset>",
        "chat": "${player}<#ff9c9c>: <#7cabee>${message}<reset>"
      }
    },
    {
      "permission": "group.donor",
      "style": {
        "displayName": "<green><bold>[<gradient:#6eca48:#aeea02>Donor</gradient><reset><green><bold>]<reset> <gold>${vanillaDisplayName}</gold><reset>",
        "chat": "${player}<white>:</white> <dark_green>${message}<reset>"
      }
    }
  ],
  "legacyChatFormatting": true,
  "parseLinksInChat": true,
  "linkStyle": "<underline><blue>${link}",
  "defaultEnabledFormatting": {
    "dark_red": false,
    "underline": false,
    "open_url": true,
    "italic": false,
    "dark_blue": false,
    "red": false,
    "aqua": false,
    "score": false,
    "run_command": false,
    "white": false,
    "dark_gray": false,
    "obfuscated": false,
    "dark_grey": false,
    "suggest_command": false,
    "item": true,
    "em": false,
    "raw": false,
    "bold": false,
    "rb": false,
    "rainbow": true,
    "reset": false,
    "style": false,
    "cmd": false,
    "copy_to_clipboard": false,
    "page": false,
    "color": false,
    "yellow": false,
    "insert": false,
    "dark_purple": false,
    "gold": false,
    "hover": false,
    "gray": false,
    "light_purple": false,
    "pos": true,
    "strikethrough": false,
    "copy": false,
    "lang": false,
    "key": false,
    "change_page": false,
    "st": false,
    "b": false,
    "green": false,
    "c": false,
    "dark_green": false,
    "gradient": false,
    "black": false,
    "i": false,
    "gr": false,
    "run_cmd": false,
    "url": false,
    "grey": false,
    "orange": false,
    "obf": false,
    "colour": false,
    "blue": false,
    "dark_aqua": false,
    "underlined": false,
    "font": false
  }
}